# Supabase Configuration
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-anon-key

# App Configuration
REACT_APP_APP_NAME=Yenege Game App
REACT_APP_APP_VERSION=1.0.0
REACT_APP_APP_ENV=development

# Google OAuth (Optional)
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id

# Payment Gateway (Optional)
REACT_APP_PAYMENT_GATEWAY_URL=your-payment-gateway-url
REACT_APP_PAYMENT_GATEWAY_KEY=your-payment-gateway-key

# Analytics (Optional)
REACT_APP_ANALYTICS_ID=your-analytics-id

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_PAYMENTS=false
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_REALTIME=true

# Development
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=info
